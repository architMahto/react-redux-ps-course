{
  "name": "react-redux-ps-project",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.7",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "reapop": "^2.0.0",
    "reapop-theme-bootstrap": "^1.0.1",
    "redux": "^4.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build-server": "webpack --config ./server.webpack.config.js",
    "dev-server": "cross-env nodemon -w ./server --ext \".*\" --exec \"cross-env PORT=5000 NODE_ENV=development babel-node server/index.js --presets babel-preset-env\"",
    "start": "npm-run-all -p watch-css start-js dev-server",
    "build": "npm-run-all build-css build-js build-server",
    "test": "react-scripts test --env=jsdom",
    "serve": "cross-env PORT=8080 NODE_ENV=production node ./build/server.bundle.js",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.10.0",
    "json-loader": "^0.5.7",
    "node-sass-chokidar": "^1.3.3",
    "nodemon": "^1.18.4",
    "redux-devtools-extension": "^2.13.5",
    "rimraf": "^2.6.2"
  },
  "proxy": {
		"/api/*": {
			"target": "http://localhost:5000"
		}
	}
}
